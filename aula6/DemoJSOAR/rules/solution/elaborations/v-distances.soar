sp {elaborate*v-distances
   (state <s>
        ^memory.ENTITY <entity>
        ^v-distances <v-distances>
        ^v-position <v-position>)
   (<v-position> ^X <px>
               ^Y <py>)
   (<entity> ^NAME <entityName>
             ^X <x>
             ^Y <y>)
-->
   (<v-distances> ^<entityName> (sqrt (+ (* (- <px> <x>) (- <px> <x>)) (* (- <py> <y>) (- <py> <y>)))))
   #(write (crlf) | elaborate*v-distances for | <entityName> ||)
   #(write (crlf) (cmd print --depth 2 <v-distances>))
}
