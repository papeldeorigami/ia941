sp {elaborate*collected-count
   (state <s>
        ^knapsack <knapsack>)
   (<knapsack> ^Red <red>
               ^Green <green>
               ^Blue <blue>
               ^Yellow <yellow>
               ^Magenta <magenta>
               ^White <white>)
-->
   (<s> ^collected-count (+ <red> <green> <blue> <yellow> <magenta> <white>))
}
