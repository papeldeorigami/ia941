sp {evaluate*no-change*propose*deliver
   (state <s> ^superstate <ss>
              ^impasse no-change
              ^all-leaflets-complete true)
-->
   (<ss> ^operator <o> +)
   (<o> ^name deliver)
   (write (crlf) | Propose deliver |)
}

sp {solution*apply*deliver
   (state <s> ^operator.name deliver)
-->
   # TODO implement the Java command to deliver leaflet
   (<s> ^delivered false -        
        ^delivered true)
   (write (crlf) | Delivered! |)
}
