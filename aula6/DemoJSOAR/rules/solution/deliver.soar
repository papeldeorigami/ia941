sp {propose*deliver
   (state <s> ^name solution
              ^ready-to-deliver true
              ^delivered false
              ^distance-to-delivery-spot { <distance> < 30 })
-->
   (<s> ^operator <o> +)
   (<o> ^name deliver)
   (write (crlf) | propose deliver |)
}

sp {solution*apply*deliver
   (state <s> ^operator.name deliver)
-->
   # TODO implement the Java command to deliver leaflet
   (<s> ^delivered false -        
        ^delivered true)
   (write (crlf) | Delivered! |)
}
