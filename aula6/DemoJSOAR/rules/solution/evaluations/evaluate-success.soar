sp {solution*evaluate*state*success
   (state <s> ^name solution
              ^desired <d>
              ^delivered true
              ^traveledDistance <traveledDistance>)
-->
   (<s> ^success <d>)
   (write (crlf) | ****************************** |)
   (write (crlf) | ********** Success! ********** |)
   (write (crlf) | traveledDistance =  | <traveledDistance> ||)
   (write (crlf) | ****************************** |)
#   (halt)
}
